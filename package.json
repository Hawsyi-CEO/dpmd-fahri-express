{
  "name": "dpmd-express-backend",
  "version": "1.0.0",
  "description": "Express.js backend for DPMD Bogor - Bumdes, Musdesus, Perjalanan Dinas",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "test": "jest",
    "db:generate": "npx prisma generate",
    "db:push": "npx prisma db push",
    "db:seed": "node scripts/run-seeders.js",
    "db:setup": "npm run db:push && npm run db:generate && npm run db:seed",
    "postinstall": "npx prisma generate"
  },
  "keywords": [
    "express",
    "bumdes",
    "musdesus",
    "dpmd"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "^6.19.0",
    "axios": "^1.13.5",
    "bcrypt": "^5.1.1",
    "bcryptjs": "^3.0.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.6.5",
    "node-cron": "^4.2.1",
    "pdfkit": "^0.17.2",
    "qrcode": "^1.5.4",
    "sequelize": "^6.35.2",
    "sharp": "^0.34.5",
    "web-push": "^3.6.7",
    "winston": "^3.11.0",
    "xlsx": "^0.18.5"
  },
